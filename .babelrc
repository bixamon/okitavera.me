{
  "env": {
    "legacy": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "corejs": 2,
            "targets": {
              "esmodules": false
            }
          }
        ]
      ]
    },
    "modern": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "corejs": 2,
            "targets": {
              "esmodules": true
            }
          }
        ]
      ]
    }
  }
}
